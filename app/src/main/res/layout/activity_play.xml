<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/backgroundlogin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InformationActivity">


    <TextView
        android:id="@+id/txt_result"
        android:layout_width="399dp"
        android:layout_height="76dp"
        android:background="@drawable/dashboardshape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/HomeButtonplay"
        android:layout_width="104dp"
        android:layout_height="69dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/dashboardshape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txt_result"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="@+id/txt_result"
        app:layout_constraintTop_toTopOf="@+id/txt_result"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_home_24"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/playButtonplay"
        android:layout_width="104dp"
        android:layout_height="69dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/dashboardshape2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txt_result"
        app:layout_constraintStart_toStartOf="@+id/txt_result"
        app:layout_constraintTop_toTopOf="@+id/txt_result"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_play_circle_24"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/iccbuttonplay"
        android:layout_width="107dp"
        android:layout_height="67dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/dashboardshape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txt_result"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/txt_result"
        app:layout_constraintTop_toTopOf="@+id/txt_result"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_information_24"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/amatirplay"
        android:layout_width="145dp"
        android:layout_height="195dp"
        android:background="@drawable/amatir"
        app:layout_constraintBottom_toTopOf="@+id/HomeButtonplay"
        app:layout_constraintEnd_toStartOf="@+id/rusunplay"
        app:layout_constraintHorizontal_bias="0.257"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.191" />

    <ImageButton
        android:id="@+id/rusunplay"
        android:layout_width="145dp"
        android:layout_height="195dp"
        android:background="@drawable/rusun"
        app:layout_constraintBottom_toTopOf="@+id/iccbuttonplay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/amatirplay"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/ojekplay"
        android:layout_width="145dp"
        android:layout_height="195dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/ojek"
        app:layout_constraintBottom_toTopOf="@+id/HomeButtonplay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amatirplay"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/kurirplay"
        android:layout_width="145dp"
        android:layout_height="195dp"
        android:background="@drawable/kurir"
        app:layout_constraintBottom_toTopOf="@+id/iccbuttonplay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rusunplay"
        app:layout_constraintVertical_bias="0.3" />


</androidx.constraintlayout.widget.ConstraintLayout>
